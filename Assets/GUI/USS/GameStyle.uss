VisualElement {
    flex-direction: row;
    align-items: flex-start;
}

.unity-image {
    --unity-image: scale-to-fit;
}

.unity-progress-bar {
}

.unity-progress-bar__container {
}

.unity-progress-bar__progress {
    background-color: rgb(74, 87, 166);
}

.unity-progress-bar__background {
    background-color: rgb(0, 0, 0);
}

.unity-progress-bar__title-container {
}

.unity-progress-bar__title {
    color: rgb(255, 255, 255);
}

#ManiteCrystal {
    --unity-image: url('project://database/Assets/Resources/Sprites/Item/shtManiteCrystal.png?fileID=-944790019&guid=91b58b58eaa42e647b54193712856e7c&type=3#shtManiteCrystal_0');
    width: 64px;
    height: 64px;
    transition-property: background-image;
    transition-timing-function: linear;
    transition-duration: 1s;
}

#HUDContainer {
    flex-direction: column;
}

#HeartContainer {
}

#Heart {
    --unity-image: url('project://database/Assets/Resources/Sprites/UI/HUD/Heart/sprHeart.png?fileID=21300000&guid=c9df5e3b9eff95241bc43a4bc789e8ba&type=3#sprHeart');
}

#EmptyHeart {
    --unity-image: url('project://database/Assets/Resources/Sprites/UI/HUD/Heart/sprEmptyHeart.png?fileID=21300000&guid=b352be7a7b72e2643a24b9384fe8e803&type=3#sprEmptyHeart');
}

#ManiteBar {
